generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Utilisateur {
  id     Int     @id @default(autoincrement())
  numero String  @unique
  nom    String?
}

model Message {
  id      Int    @id @default(autoincrement())
  contenu String
}
